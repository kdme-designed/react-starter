{
  "name": "react-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix .src/**.{js,jsx,ts,tsx}",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.1.0",
    "@vanilla-extract/vite-plugin": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "4.3.5",
    "vite": "^2.3.8",
    "vite-plugin-svgr": "^0.3.0",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript-prettier"
    ],
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "volta": {
    "node": "16.4.1",
    "npm": "7.19.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  }
}
